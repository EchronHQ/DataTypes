{
  "name": "echron/datatypes",
  "description": "Extra basic datatypes and helper classes",
  "homepage": "https://github.com/echron/datatypes",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Stijn Duynslaeger"
    }
  ],
  "require": {
    "php": ">=7.4 || >=8.0",
    "echron/tools": "^1.1",
    "ext-json": "*"
  },
  "autoload": {
